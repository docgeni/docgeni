{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc/site",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "importHelpers": true,
    "target": "ES2022",
    "useDefineForClassFields": false,
    "noImplicitOverride": true,
    "strict": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "types": [
      "node"
    ],
    "paths": {
      "@docgeni/template": [
        "../../packages/template/src/public-api.ts"
      ],
      "@docgeni/template/*": [
        "../../packages/template/src/*"
      ],
      "@docgeni/alib": [
        "../../packages/a-lib/public-api.ts"
      ],
      "@docgeni/alib/*": [
        "../../packages/a-lib/*"
      ]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/assets",
    "test.ts"
  ]
}

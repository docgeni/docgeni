{
  "packages": [
    "packages/toolkit",
    "packages/core",
    "packages/cli",
    "packages/template"
  ],
  "command": {
    "version": {
      "conventionalCommits": true,
      "gitTagVersion": false,
      "push": false,
      "yes": true
    },
    "publish": {
      "ignoreChanges": [
        "*.md"
      ],
      "verifyAccess": false,
      "verifyRegistry": false,
      "message": "chore: release %s"
    }
  },
  "npmClient": "yarn",
  "version": "1.0.0-beta.3",
  "useWorkspaces": true
}

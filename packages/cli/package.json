{
  "name": "@docgeni/cli",
  "version": "1.0.0-beta.3",
  "description": "CLI tool for Docgeni",
  "author": "why520crazy <why520crazy@163.com>",
  "homepage": "https://github.com/docgeni/docgeni#readme",
  "license": "ISC",
  "main": "index.js",
  "bin": {
    "docgeni": "bin/docgeni"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docgeni/docgeni.git"
  },
  "scripts": {
    "clean": "rimraf ./lib && rimraf ./tsconfig.tsbuildinfo",
    "build-only": "tsc -b",
    "build": "yarn clean && yarn build-only && yarn copy:schematics",
    "copy:schematics": "cpx \"./src/schematics/**/{.*,*}.{json,template}\" ./lib/schematics/ -v",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/docgeni/docgeni/issues"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular-devkit/schematics": "~10.2.1",
    "@angular-devkit/schematics-cli": "^0.1002.3",
    "@angular/animations": "~10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/cli": "~10.2.1",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@docgeni/core": "^1.0.0-beta.3",
    "@docgeni/template": "^1.0.0-beta.3",
    "@docgeni/toolkit": "^1.0.0-beta.3",
    "@schematics/angular": "^10.0.0",
    "chokidar": "^3.3.1",
    "cosmiconfig": "^6.0.0",
    "fancy-log": "^1.3.3",
    "front-matter": "^3.1.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "marked": "^0.8.1",
    "rxjs": "^6.5.4",
    "stringify-object": "^3.3.0",
    "tapable": "^1.1.3",
    "tslib": "^2.0.0",
    "yargs": "15.3.1",
    "zone.js": "^0.10.2"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": ">=0.900.0 <0.1200.0",
    "@angular/animations": ">=9.0.0 <12.0.0",
    "@angular/cdk": ">=9.0.0 <12.0.0",
    "@angular/cli": ">=9.0.0 <12.0.0",
    "@angular/common": ">=9.0.0 <12.0.0",
    "@angular/compiler": ">=9.0.0 <12.0.0",
    "@angular/compiler-cli": ">=9.0.0 <12.0.0",
    "@angular/core": ">=9.0.0 <12.0.0",
    "@angular/forms": ">=9.0.0 <12.0.0",
    "@angular/platform-browser": ">=9.0.0 <12.0.0",
    "@angular/platform-browser-dynamic": ">=9.0.0 <12.0.0",
    "@angular/router": ">=9.0.0 <12.0.0",
    "rxjs": ">=6.5.4",
    "tslib": ">=2.0.0",
    "zone.js": ">=0.10.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/stringify-object": "^3.3.0",
    "@types/tapable": "^1.0.5",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "typescript": "4.0.5"
  },
  "schematics": "./lib/schematics/collection.json",
  "ng-add": {
    "save": "devDependencies"
  }

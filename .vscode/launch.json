    "version": "0.2.0",
    "configurations": [
      {
        "command": "yarn start:docs --skip-site",
        "name": "Run build docs",
        "request": "launch",
        "type": "node-terminal",
        "skipFiles": [
          "<node_internals>/**",
          "node_modules/**"
        ],
      },
      {
        "command": "yarn test",
        "name": "Run Test",
        "request": "launch",
        "type": "node-terminal",
        "skipFiles": [
          "<node_internals>/**",
          "node_modules/**"
        ],
      }
    ]
}
